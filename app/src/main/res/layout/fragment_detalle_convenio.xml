<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.example.foxconntech.egresoft.fragments.DetalleConvenio">

    <ImageView
        android:id="@+id/fotoDetalleconvenio"
        android:layout_width="wrap_content"
        android:layout_height="250dp" />

    <TextView
        android:id="@+id/nombreDetalleConvenio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/infoDetalleConvenio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/contactoDetalleConvenio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>
